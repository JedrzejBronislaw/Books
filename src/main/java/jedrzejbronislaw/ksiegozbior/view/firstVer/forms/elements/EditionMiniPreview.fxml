<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>


<fx:root type="GridPane" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="%title" />
      <Label text="%publishing_house" GridPane.rowIndex="3" />
      <Label text="%author" GridPane.rowIndex="1" />
      <Label text="%language" GridPane.rowIndex="2" />
      <Label text="%issue_number" GridPane.rowIndex="4" />
      <Label fx:id="editionTitle" text="Label" GridPane.columnIndex="1" />
      <Label fx:id="editionAuthor" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label fx:id="editionLanguage" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label fx:id="editionPublisher" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Label fx:id="editionNumber" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
   </children>
</fx:root>
